CREATE TABLE `customer` (
  `CUSTOMER_ID` int(11) NOT NULL,
  `USERNAME` varchar(16) NOT NULL,
  `PASSWORD` varchar(10) NOT NULL,
  `TRAN_PASSWORD` varchar(10) NOT NULL,
  `C_FIRSTNAME` varchar(40) NOT NULL,
  `C_LASTNAME` varchar(40) NOT NULL,
  `C_MIDDLENAME` varchar(40) DEFAULT NULL,
  `C_ADDRESS1` varchar(60) NOT NULL,
  `C_ADDRESS2` varchar(60) DEFAULT NULL,
  `C_CITY` varchar(30) NOT NULL,
  `C_ZIP` varchar(10) NOT NULL,
  `C_STATE` char(2) NOT NULL,
  `C_EMAIL` varchar(40) NOT NULL,
  `C_PHONENUMBER` int(11) NOT NULL,
  `BRANCH_CODE` char(4) NOT NULL,
  PRIMARY KEY (`CUSTOMER_ID`),
  KEY `BRANCH_CODE` (`BRANCH_CODE`),
  CONSTRAINT `customer_ibfk_1` FOREIGN KEY (`BRANCH_CODE`) REFERENCES `branch` (`BRANCH_CODE`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
