CREATE TABLE `transaction_history` (
  `TRANSACTION_ID` int(11) NOT NULL,
  `BRANCH_CODE` char(4) NOT NULL,
  `ACC_NO` char(8) NOT NULL,
  `TRANSTYPE_ID` char(4) NOT NULL,
  `TRANS_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `TRANS_DESC` varchar(40) DEFAULT NULL,
  `TRANS_AMOUNT` decimal(10,2) NOT NULL,
  PRIMARY KEY (`TRANSACTION_ID`),
  KEY `fk_transaction_history_1_idx` (`ACC_NO`),
  KEY `fk_transaction_history_2_idx` (`TRANSTYPE_ID`),
  KEY `fk_transaction_history_3_idx` (`BRANCH_CODE`),
  CONSTRAINT `fk_transaction_history_1` FOREIGN KEY (`ACC_NO`) REFERENCES `account` (`ACC_NUMBER`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_transaction_history_2` FOREIGN KEY (`TRANSTYPE_ID`) REFERENCES `transaction_type` (`TRANSTYPE_ID`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `fk_transaction_history_3` FOREIGN KEY (`BRANCH_CODE`) REFERENCES `branch` (`BRANCH_CODE`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
