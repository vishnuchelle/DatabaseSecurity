CREATE TABLE `auth_number` (
  `CUSTOMER_ID` int(11) NOT NULL,
  `AUTH_NUMBER` int(11) NOT NULL,
  KEY `fk_auth_number_1_idx` (`CUSTOMER_ID`),
  CONSTRAINT `fk_auth_number_1` FOREIGN KEY (`CUSTOMER_ID`) REFERENCES `customer` (`CUSTOMER_ID`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
